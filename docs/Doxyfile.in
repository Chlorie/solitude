INPUT = @PROJECT_SOURCE_DIR@/lib/include/solitude
RECURSIVE = YES
GENERATE_HTML = NO
GENERATE_LATEX = NO
GENERATE_XML = YES
WARN_IF_UNDOCUMENTED = NO
ALIASES = "rst=\verbatim embed:rst:leading-asterisk"
